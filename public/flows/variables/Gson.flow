attributes:
  ArrowStyle: ArrowEnd
  NodeStyle: BoxIcon
  PROCESS_VISIBILITY: PUBLIC
  display: x=37,y=31,w=51,h=26
  requestMethod: POST
  requestParameters: '[["mountain","Body","true","","tests.variables/Mountain.java"]]'
  requestPath: Gson
  requestSummary: Because it's there
steps:
- id: S1
  name: Start
  specifier: flowbee-demo.start
  attributes:
    display: x=70,y=110,w=60,h=40
  links:
  - id: L1
    attributes:
      display: type=Elbow,x=175,y=130,xs=134&216,ys=130&130
    event: FINISH
    to: S4
- id: S2
  name: Stop
  specifier: flowbee-demo.stop
  attributes:
    display: x=640,y=110,w=60,h=40
- id: S3
  name: Further Updates
  specifier: flowbee-demo.java.DynamicJavaActivity
  attributes:
    ClassName: Gson_A3
    Java: ""
    Output Documents: '["input","local","request"]'
    display: x=430,y=90,w=120,h=80
  links:
  - id: L4
    attributes:
      display: type=Elbow,x=595,y=130,xs=554&636,ys=130&130
    event: FINISH
    to: S2
- id: S4
  name: Create/Update
  specifier: flowbee-demo.script.ScriptExecutorActivity
  attributes:
    Output Documents: '["input","local","request"]'
    Rule: |+
      import tests.variables.Mountain

      if (testCase == null)
          testCase = "Process Request"
      if (request != null) {
          request.elevation = request.elevation - 1 // rising sea level
      }
      if (input != null) {
          input.elevation = input.elevation - 1
      }
      local = new Mountain("Fuji", 12389, "Japan")


    SCRIPT: Groovy
    display: x=220,y=90,w=120,h=80
  links:
  - id: L3
    attributes:
      display: type=Elbow,x=385,y=130,xs=344&426,ys=130&130
    event: FINISH
    to: S3
variables:
  input:
    type: com.google.gson.Gson
  local:
    type: com.google.gson.Gson
  request:
    type: com.google.gson.Gson
  requestHeaders:
    type: java.util.Map<String,String>
  testCase:
    type: java.lang.String
