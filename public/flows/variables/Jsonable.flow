attributes:
  ArrowStyle: ArrowEnd
  NodeStyle: BoxIcon
  PROCESS_VISIBILITY: PUBLIC
  display: x=44,y=39,w=84,h=26
  requestMethod: POST
  requestParameters: '[["mountain","Body","true","","tests.variables/Mountain.java"]]'
  requestPath: Jsonable
  requestSummary: Because it's there
steps:
- id: S1
  name: Start
  path: flowbee-demo.start
  attributes:
    display: x=70,y=130,w=60,h=40
  links:
  - id: L1
    attributes:
      display: type=Elbow,x=175,y=150,xs=134&216,ys=150&150
    to: S4
- id: S2
  name: Stop
  path: flowbee-demo.stop
  attributes:
    display: x=670,y=130,w=60,h=40
- id: S4
  name: Create/Update
  path: flowbee-demo.script.ScriptExecutorActivity
  attributes:
    Output Documents: '["input","local","request"]'
    Rule: |+
      import tests.variables.Mountain

      if (testCase == null)
          testCase = "Process Request"
      if (request != null) {
          request.elevation = request.elevation - 1 // rising sea level
      }
      if (input != null) {
          input.elevation = input.elevation - 1
      }
      local = new Mountain("Fuji", 12389, "Japan")


    SCRIPT: Groovy
    display: x=220,y=110,w=120,h=80
  links:
  - id: L5
    attributes:
      display: type=Elbow,x=385,y=150,xs=344&426,ys=150&150
    to: S6
- id: S6
  name: Further Updates
  path: flowbee-demo.java.DynamicJavaActivity
  attributes:
    ClassName: Jsonable_A6
    Java: ""
    Output Documents: '["input","local","request"]'
    display: x=430,y=110,w=120,h=80
  links:
  - id: L4
    attributes:
      display: type=Elbow,x=610,y=150,xs=554&666,ys=150&150
    to: S2
variables:
  input:
    type: io.limberest.json.Jsonable
  local:
    type: io.limberest.json.Jsonable
  request:
    type: io.limberest.json.Jsonable
  requestHeaders:
    type: java.util.Map<String,String>
  testCase:
    type: java.lang.String
