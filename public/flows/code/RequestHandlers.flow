  links:
  - id: L2
    attributes:
      display: type=Elbow,x=450,y=130,xs=384&516,ys=130&130
    event: FINISH
    to: S2
variables:
  notifyMessage:
    type: org.json.JSONObject
  request:
    type: org.json.JSONObject
