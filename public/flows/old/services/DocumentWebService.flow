attributes:
  ArrowStyle: ArrowEnd
  NodeStyle: BoxIcon
  PROCESS_VISIBILITY: PUBLIC
  SLA: '20'
  SLA_UNIT: Minutes
  Transition with no Label: Acts as Default
  display: x=30,y=29,w=200,h=27
steps:
- id: s1
  name: Start
  path: flowbee-demo.start
  attributes:
    display: x=72,y=121,w=60,h=40
  links:
  - id: l1
    attributes:
      display: type=Elbow,x=193,y=118,xs=136&234,ys=141&141
    to: s3
- id: s2
  name: Stop
  path: flowbee-demo.stop
  attributes:
    display: x=487,y=120,w=60,h=40
- id: s3
  name: "Invoke\r\nEmployee Service"
  path: flowbee-demo.adapter.soap.DocumentWebServiceAdapter
  attributes:
    DO_LOGGING: 'True'
    REQUEST_VARIABLE: requestDoc
    RESPONSE_VARIABLE: responseDoc
    WSDL: '#{props[''services.url'']}/SOAP/Employee.wsdl'
    display: x=238,y=112,w=118,h=71
    max_tries: '3'
    retry_interval: '30'
  links:
  - id: l2
    attributes:
      display: type=Elbow,x=422,y=140,xs=360&483,ys=140&140
    to: s2
variables:
  requestDoc:
    type: org.apache.xmlbeans.XmlObject
  responseDoc:
    type: org.w3c.dom.Document
