attributes:
  ArrowStyle: ArrowEnd
  NodeStyle: BoxIcon
  PROCESS_VISIBILITY: SERVICE
  PerformanceLevel: '9'
  SLA: '20'
  SLA_UNIT: Minutes
  Transition with no Label: Acts as Default
  display: x=50,y=50,w=185,h=27
steps:
- id: S1
  name: Start
  descriptor: flowbee-demo.start
  attributes:
    display: x=56,y=261,w=60,h=40
  links:
  - id: L1
    attributes:
      display: type=Elbow,x=126,y=281,xs=120&153,ys=281&281
    event: FINISH
    to: S5
- id: S2
  name: Stop
  descriptor: flowbee-demo.stop
  attributes:
    display: x=561,y=125,w=60,h=40
- id: S3
  name: Invoke regular Async Subprocess
  descriptor: flowbee-demo.process.InvokeSubProcessActivity
  attributes:
    display: x=323,y=115,w=183,h=59
    processname: tests.workflow/SmartProcessChild
    processversion: '[2.3,3)'
    synchronous: 'FALSE'
    variables: '{"inputVar":"#{myvar}"}'
  links:
  - id: L2
    attributes:
      display: type=Elbow,x=540,y=144,xs=510&557,ys=145&145
    event: FINISH
    to: S2
- id: S4
  name: Set document
  descriptor: flowbee-demo.script.ScriptExecutorActivity
  attributes:
    Output Documents: myvar
    Rule: myvar="Some special variable value in here!";
    SCRIPT: Groovy
    display: x=168,y=117,w=100,h=60
  links:
  - id: L3
    attributes:
      display: type=Elbow,x=306,y=148,xs=272&319,ys=145&145
    event: FINISH
    to: S3
- id: S5
  name: Which Test Case?
  descriptor: flowbee-demo.script.ScriptEvaluator
  attributes:
    Expression: processCase
    SCRIPT: Groovy
    display: x=157,y=250,w=122,h=61
  links:
  - id: L4
    attributes:
      display: type=Elbow,x=118,y=207,xs=218&218,ys=246&181
    event: FINISH
    to: S4
    result: DocumentBinding
variables:
  myvar:
    type: model.StringDocument
  processCase:
    type: java.lang.String
