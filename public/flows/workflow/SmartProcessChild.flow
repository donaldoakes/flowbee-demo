attributes:
  NodeStyle: BoxIcon
  PerformanceLevel: '0'
  display: x=32,y=16,w=172,h=27
steps:
- id: S1
  name: Start
  specifier: com.centurylink.mdw.workflow.activity.process.ProcessStartActivity
  attributes:
    display: x=60,y=93,w=60,h=40
  links:
  - id: L1
    attributes:
      display: type=Elbow,x=172,y=112,xs=124&220,ys=112&112
    event: FINISH
    to: S3
- id: S2
  name: Stop
  specifier: com.centurylink.mdw.workflow.activity.process.ProcessFinishActivity
  attributes:
    display: x=423,y=94,w=60,h=40
- id: S3
  name: Set Info
  specifier: com.centurylink.mdw.workflow.activity.script.ScriptExecutorActivity
  attributes:
    Monitors: '[["true","Milestone","com.centurylink.mdw.milestones/ActivityMilestone.java","sub ${inputVar}"],["false","Test Cases","com.centurylink.mdw.testing/TestCaseActivityMonitor.java",""],["false","Zipkin Activity","com.centurylink.mdw.zipkin/TraceActivityMonitor.java",""]]'
    Rule: "procName = runtimeContext.getProcess().getName();\r\nprocVer = runtimeContext.getProcess().getVersionString();"
    SCRIPT: Groovy
    display: x=224,y=85,w=100,h=60
  links:
  - id: L2
    attributes:
      display: type=Elbow,x=373,y=114,xs=328&418,ys=114&114
    event: FINISH
    to: S2
variables:
  inputVar:
    type: java.lang.String
  procName:
    type: java.lang.String
  procVer:
    type: java.lang.String
