attributes:
  ArrowStyle: ArrowEnd
  NodeStyle: BoxIcon
  PROCESS_VISIBILITY: PUBLIC
  display: x=50,y=50,w=0,h=0
steps:
- id: S1
  name: Start
  path: flowbee-demo.start
  attributes:
    display: x=60,y=260,w=60,h=40
  links:
  - id: L1
    attributes:
      display: type=Elbow,x=177,y=280,xs=124&230,ys=280&280
    to: S3
- id: S2
  name: Stop
  path: flowbee-demo.stop
  attributes:
    display: x=480,y=260,w=60,h=40
- id: S3
  name: Force Error
  path: flowbee-demo.script.ScriptExecutorActivity
  attributes:
    Rule: |-
      runtimeContext.logDebug("Executing " + runtimeContext.getActivity().getName())
      throw new RuntimeException("Help!")
    SCRIPT: Groovy
    display: x=233,y=250,w=100,h=60
  links:
  - id: L2
    attributes:
      display: type=Elbow,x=407,y=280,xs=338&476,ys=280&280
    to: S2
subflows:
- id: F1
  name: Exception Handler
  attributes:
    EMBEDDED_PROCESS_TYPE: Exception Handler
    PROCESS_VISIBILITY: EMBEDDED
    display: x=399,y=58,w=440,h=120
  steps:
  - id: S4
    name: Start
    path: flowbee-demo.start
    attributes:
      display: x=439,y=98,w=60,h=40
    links:
    - id: L3
      attributes:
        display: type=Elbow,x=533,y=118,xs=503&563,ys=118&118
      to: S7
  - id: S6
    name: Stop
    path: flowbee-demo.stop
    attributes:
      display: x=739,y=98,w=60,h=40
  - id: S7
    name: "Subprocess \nfrom Embedded"
    path: flowbee-demo.process.InvokeSubProcessActivity
    attributes:
      display: x=568,y=87,w=105,h=66
      processname: tests.workflow/SubprocessForEmbedded.proc
      processversion: '[0.4,1)'
      synchronous: 'true'
      variables: '{"fromParent":"$passToSub","toParent":"$receiveFromSub"}'
    links:
    - id: L4
      attributes:
        display: type=Elbow,x=705,y=118,xs=676&735,ys=118&118
      to: S6
variables:
  passToSub:
    type: java.lang.String
  receiveFromSub:
    type: java.lang.String
