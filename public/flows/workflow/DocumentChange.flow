    Output Documents: changedJavaObject#unchangedJavaObject
    Rule: ""
    SCRIPT: Groovy
    display: x=135,y=143,w=110,h=70
  links:
  - id: L4
    attributes:
      display: type=Elbow,x=275,y=171,xs=249&300,ys=171&171
