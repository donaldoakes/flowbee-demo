    to: S3
- id: S2
  name: Stop
  specifier: com.centurylink.mdw.workflow.activity.process.ProcessFinishActivity
  attributes:
    display: x=537,y=175,w=60,h=40
- id: S3
  name: |-
    Create
    Request
  specifier: com.centurylink.mdw.workflow.activity.script.ScriptExecutorActivity
  attributes:
    Output Documents: '["serviceRequest"]'
    Rule: |-
      // empty request is okay
      serviceRequest = new org.json.JSONObject()
