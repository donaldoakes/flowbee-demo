- id: S1
  name: Start
  specifier: com.centurylink.mdw.workflow.activity.process.ProcessStartActivity
  attributes:
    display: x=69,y=171,w=60,h=40
  links:
  - id: L1
    attributes:
      display: type=Elbow,x=158,y=191,xs=133&183,ys=191&191
