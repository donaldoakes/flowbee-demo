- id: S4
  name: |-
    POST
    404
  specifier: com.centurylink.mdw.workflow.adapter.rest.RestServiceAdapter
  attributes:
    EndpointUri: '#{props[''mdw.services.url'']}/api/notfound'
    HttpMethod: POST
    REQUEST_VARIABLE: serviceRequest
    RESPONSE_VARIABLE: serviceResponse
    display: x=356,y=156,w=112,h=74
  links:
  - id: L3
    attributes:
      display: type=Elbow,x=502,y=195,xs=472&533,ys=195&195
